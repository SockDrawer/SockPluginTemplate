{
  "name": "sockplugintemplate",
  "version": "1.0.0",
  "description": "A barebones template for designing a sockbot plugin",
  "main": "src/plugin.js",
  "scripts": {
    "start": "node --harmony --harmony_arrow_functions node_modules/SockBot/lib/cli.js",
    "lint": "eslint .",
    "test": "eslint . && istanbul cover node_modules/mocha/bin/_mocha -- --recursive -R dot ",
    "istanbul": "eslint .; istanbul cover node_modules/mocha/bin/_mocha -- --recursive -R nyan",
    "mocha": "eslint .;mocha --recursive -R nyan"
  },
  "author": "yamikuronue",
  "license": "MIT",
  "dependencies": {
    "sockbot": "^2.11.9"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "eslint": "^1.10.3",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2",
    "sinon-as-promised": "^4.0.0"
  }
}
